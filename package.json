{
  "name": "notiondown",
  "version": "0.2.2",
  "description": "A CLI tool and Node.js library to convert Notion pages to markdown and HTML with cache support.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "dev:ts": "node src/cli.ts",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "notiondown": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rot1024/notiondown.git"
  },
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rot1024/notiondown/issues"
  },
  "homepage": "https://github.com/rot1024/notiondown#readme",
  "dependencies": {
    "@microflash/rehype-figure": "^2.1.4",
    "@notionhq/client": "^5.3.0",
    "@remark-embedder/core": "^3.0.3",
    "@remark-embedder/transformer-codesandbox": "^3.0.0",
    "@remark-embedder/transformer-oembed": "^5.0.1",
    "@supercharge/promise-pool": "^3.2.0",
    "add": "^2.0.6",
    "async-retry": "^1.3.3",
    "commander": "^14.0.2",
    "hast": "^0.0.2",
    "hastscript": "^9.0.1",
    "is-url": "^1.2.4",
    "mdast": "^2.3.2",
    "notion-to-md": "^3.1.9",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sharp": "^0.34.4",
    "shiki": "^3.14.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/is-url": "^1.2.32",
    "@types/node": "^24.9.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  }
}
